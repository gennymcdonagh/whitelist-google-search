{
  "manifest_version": 2,
  "name": "Whitelist Google Search",
  "version": "0.1",
  "background": {"scripts":["background.js"]},
  "permissions": [
    "webRequest",
    "https://www.google.com/search?*",
    "http://www.google.com/search?*",
    "webRequestBlocking"
  ],
  "browser_action": {
    "default_icon": {                    
      "16": "images/icon16.png"         
    },
    "default_title": "Whitelist Google Search"
  }
}